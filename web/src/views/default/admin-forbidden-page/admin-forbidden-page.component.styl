.wrapper {
  height: 100vh
  width: 100%
  --background-color: var(--pal-accent)
  --accent-color: var(--pal-base-03)
  --accent-alt: var(--pal-base-03)
  --alt-primary: var(--pal-base-04)
  display: flex
  flex-direction: column
  align-items: stretch
  background: var(--background-color)
}

.notfound-wrapper {
  flex-grow: 1
  display: flex
  flex-direction: column
  align-items: center
  justify-content: center
  text-align: center
}

